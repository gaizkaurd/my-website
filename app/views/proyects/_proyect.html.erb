<div id="<%= dom_id proyect %>">
  <p>
    <strong>Name:</strong>
    <%= proyect.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= proyect.description %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= proyect.content %>
  </p>

  <p>
    <strong>Repo:</strong>
    <%= proyect.repo %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= proyect.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= proyect.end_time %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= proyect.active %>
  </p>

</div>
